<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.appiii">
    <!-- 粗略定位授權 -->
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" /> <!-- 精細定位授權 -->
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" /> <!-- 網路權限 -->
    <uses-permission android:name="android.permission.INTERNET" /> <!-- 網路訪問授權 -->
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" /> <!-- GPS權限 -->
    <uses-feature android:name="android.hardware.location.gps" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity
            android:name=".ActBottom"
            android:label="@string/title_activity_act_bottom">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>
        <activity android:name=".ActDBconect" />
        <activity android:name=".ActGps" />
        <activity android:name=".ActMain">

        </activity>
    </application>

</manifest>